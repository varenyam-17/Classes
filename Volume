#include<iostream>
using namespace std;
class Calc
{
    public:
    int length;
    int breadth;
    int height;
    void vol(double length,double breadh,double height)
    {
       
        int vol=length*breadth*height;
        
        cout<<"Volume: "<<vol;
    }
};
void input()
{
    Calc c1;

    cout << "Enter length: ";
    cin >> c1.length;
    cout << "Enter breadth: ";
    cin >> c1.breadth;
    cout << "Enter height: ";
    cin >> c1.height;
    c1.vol(c1.length,c1.breadth,c1.height);
   
}
int main() {
    input();
}
